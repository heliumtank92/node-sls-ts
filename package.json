{
  "name": "node-sls-ts",
  "version": "0.0.1",
  "description": "Node Boilerplate using Express JS. This Boilerplate supports Jose Crypto, Logging Solution & HttpClient by default.",
  "scripts": {
    "lint": "tsc --noEmit && eslint src/* --quiet",
    "lint:fix": "eslint src/* --fix --quiet",
    "compile": "tsc",
    "build:clean": "rm -rf ./dist",
    "build": "npm run build:clean && npm run compile",
    "test": "echo \"Error: no test specified\"",
    "start:dev": "export NODE_ENV=development && nodemon src/app.ts",
    "start": "export NODE_ENV=production && node dist/app.js"
  },
  "dependencies": {
    "@am92/api-logger": "^2.0.4",
    "@am92/express-utils": "^2.0.10",
    "@am92/node-http": "^2.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.13",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "nodemon": "^3.1.2",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "repository": "git@github.com:heliumtank92/node-sls-ts.git",
